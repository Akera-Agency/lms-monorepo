{
  "name": "elysia-app",
  "version": "1.0.0",
  "type": "module",
  "main": "src/app.ts",
  "scripts": {
    "dev": "bun --bun run --watch src/app.ts",
    "build": "bun build src/app.ts --outfile dist/app.js --target bun",
    "start": "bun run dist/app.js",
    "lint": "bun run --bun eslint . --ext .ts",
    "format": "bun run --bun prettier --write .",
    "----------------------i18n------------------------------": "------------------------------------------------",
    "generate:i18n-types": "bun run src/shared/i18n/generate-types.ts",
    "--------------------migrations------------------------------": "------------------------------------------------",
    "migration:run": "bun run --bun src/database/runners/runMigrations.ts",
    "migration:create": "bun run --bun src/database/runners/create-migration.ts",
    "migration:to": "bun run --bun src/database/runners/migrateTo.ts",
    "seed": "bun run --bun src/database/runners/seed.ts",
    "migration:list": "bun run --bun src/database/runners/listAppliedMigrations.ts"
  },
  "peerDependencies": {
    "elysia": "^1.4.5",
    "zod": "^3.23.8"
  },
  "dependencies": {
    "@akera/env": "workspace:*",
    "@aws-sdk/client-s3": "^3.658.1",
    "@aws-sdk/s3-request-presigner": "^3.658.1",
    "@bogeychan/elysia-logger": "^0.1.10",
    "@react-email/render": "^1.2.3",
    "@sentry/bun": "10.10.0",
    "elysia-requestid": "1.0.9",
    "eventemitter2": "^6.4.9",
    "i18next": "^23.7.6",
    "i18next-fs-backend": "^2.3.1",
    "i18next-http-middleware": "^3.5.0",
    "jsonwebtoken": "^9.0.2",
    "kysely": "^0.28.7",
    "pg": "^8.16.3",
    "pino-pretty": "^13.1.1",
    "posthog-node": "5.8.2",
    "resend": "^6.0.2",
    "uuid": "^10.0.0"
  },
  "devDependencies": {
    "elysia": "^1.4.5",
    "zod": "^3.23.8",
    "typescript": "5.6.3",
    "@types/node": "^18.19.124",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/uuid": "^10.0.0",
    "bun-types": "^1.2.21"
  }
}
